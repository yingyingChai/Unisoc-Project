.dropdown .dropdown-menu {
	width: 100%;
	z-index: 1000;
}

.table.table-hover tbody tr {
	background: none;
}
	.table.table-hover tbody tr.isPending,.table.table-hover tbody tr.isPending a {
		background-color:#fcff00;
		color: #000;
	}


	.table.table-hover tbody tr.isRelease,.table.table-hover tbody tr.isRelease a {
		background-color:#b6ffb9;
		color: #000;
	}
	.table.table-hover tbody tr.isBin1Release,.table.table-hover tbody tr.isBin1Release a {
		background-color: #b1c9ff;
		color: #000;
	}
	.table.table-hover tbody tr.isScrap,.table.table-hover tbody tr.isScrap a  {
		background-color: #d8d8d8;
		color: #000;
	}
	.table.table-hover tbody tr.isRescreen,.table.table-hover tbody tr.isRescreen a{
		background-color: #fffbc0;
		color: #000;
	}
	.table.table-hover tbody tr.isHold ,.table.table-hover tbody tr.isHold a {
		background-color: #ffbcb7;
		color: #000;
	}
     .table.table-hover tbody tr.isOperationHold,.table.table-hover tbody tr.isOperationHold a {
		background-color: #ffe599;
		
	}
	.table.table-hover tbody tr.isclick,.table.table-hover tbody tr.isclick a{
		background-color: #025e9d;
		color: #fff;
	}
    
    
