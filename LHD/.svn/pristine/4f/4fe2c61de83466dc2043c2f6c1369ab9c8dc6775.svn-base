using System;
namespace KaYi.Web.Infrastructure.Model.System.DictItems
{
	public class DictItem
	{
		private string _ItemID = string.Empty;
		private string _Title = string.Empty;
		private string _ItemType = string.Empty;
		private int _Sequence;
		private string _Introduction = string.Empty;
		private string _ParentItemID = string.Empty;
		private int _Grade;
		private int subItemCount;
		public int SubItemCount
		{
			get
			{
				return this.subItemCount;
			}
			set
			{
				this.subItemCount = value;
			}
		}
		public string ItemID
		{
			get
			{
				return this._ItemID;
			}
			set
			{
				this._ItemID = value.Trim();
			}
		}
		public string Title
		{
			get
			{
				return this._Title;
			}
			set
			{
				this._Title = value.Trim();
			}
		}
		public string ItemType
		{
			get
			{
				return this._ItemType;
			}
			set
			{
				this._ItemType = value.Trim();
			}
		}
		public int Sequence
		{
			get
			{
				return this._Sequence;
			}
			set
			{
				this._Sequence = value;
			}
		}
		public string Introduction
		{
			get
			{
				return this._Introduction;
			}
			set
			{
				this._Introduction = value.Trim();
			}
		}
		public string ParentItemID
		{
			get
			{
				return this._ParentItemID;
			}
			set
			{
				this._ParentItemID = value.Trim();
			}
		}
		public int Grade
		{
			get
			{
				return this._Grade;
			}
			set
			{
				this._Grade = value;
			}
		}
	}
}
